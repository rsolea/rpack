#!/usr/bin/env ruby

require "rubygems"
require "yaml"
require "zip/zip"
require "fileutils"
require "#{File.expand_path(File.dirname(__FILE__))}/../lib/parser.rb"
require "#{File.expand_path(File.dirname(__FILE__))}/../lib/rpack.rb"

parser   = Rpack::Parser.new(ARGV)
rpack    = Rpack::Rpack.new(ARGV,parser)
rpack.run
